<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.sistemab.InfoprestadoController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" layoutX="12.0" layoutY="12.0" prefHeight="400.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" prefHeight="53.0" prefWidth="370.0" text="Libros Prestados" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="36.0" />
                     </font>
                  </Label>
                  <Label />
                  <HBox alignment="CENTER">
                     <children>
                        <Label prefHeight="17.0" prefWidth="118.0" text="USUARIO:" />
                        <Label fx:id="lblnombre" prefHeight="17.0" prefWidth="230.0" text="Nombre" />
                     </children>
                  </HBox>
                  <Label />
                  <TableView fx:id="loslibros" prefHeight="284.0" prefWidth="600.0">
                     <columns>
                        <TableColumn fx:id="coltitulo" prefWidth="213.0" text="Título" />
                        <TableColumn fx:id="colautor" prefWidth="166.0" text="Autor" />
                        <TableColumn fx:id="colemision" prefWidth="108.0" text="Fecha Emisión" />
                        <TableColumn fx:id="colDevolucion" prefWidth="112.0" text="Fecha Devolución" />
                     </columns>
                  </TableView>
                  <Label />
                  <HBox>
                     <children>
                        <Label prefHeight="17.0" prefWidth="57.0" />
                        <Button mnemonicParsing="false" onAction="#regresar" text="Regresar" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</HBox>
